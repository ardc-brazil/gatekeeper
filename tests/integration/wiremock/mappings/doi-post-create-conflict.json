{
  "request": {
    "method": "POST",
    "url": "/dois",
    "headers": {
      "Content-Type": {
        "equalTo": "application/vnd.api+json"
      },
      "Authorization": {
        "matches": "Basic .*"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.data.type",
        "equalTo": "dois"
      },
      {
        "matchesJsonPath": "$.data.attributes.doi",
        "equalTo": "10.82978/DUPLICATE123"
      }
    ]
  },
  "response": {
    "status": 409,
    "headers": {
      "Content-Type": "application/vnd.api+json"
    },
    "body": "{\"errors\":[{\"status\":\"409\",\"title\":\"DOI already exists\",\"detail\":\"A DOI with this identifier already exists.\"}]}"
  }
}
