{
  "request": {
    "method": "GET",
    "urlPathPattern": "/dois/([^/]+)/([^/]+)",
    "headers": {
      "Authorization": {
        "matches": "Basic .*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/vnd.api+json"
    },
    "bodyFileName": "doi-get-response.json",
    "transformers": ["response-template"]
  }
}
